{
	"presets": [
		"es2015",
		"stage-0",
		"react"
	],
	"plugins": [
		[
			"transform-decorators-legacy"
		],
		[
			"transform-runtime", 
			{
				"polyfill": false,
				"regenerator": true
			}
		],
		[
			"import",
			[
				{
					"libraryName": "antd",
					"style": true
				},
				{ 
					"libraryName": "antd-mobile",
					"style": true
				}
			]
		]
		
	]
}
